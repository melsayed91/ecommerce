{
  "name": "Shipment",
  "base": "PersistedModel",
  "plural":"shipments",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "awb": {
      "type": "string"
    },
    "shippedBy": {
      "type": "string"
    },
    "shippingFees": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
