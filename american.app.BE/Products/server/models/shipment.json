    },
    "seller": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "sellerId"
    },
    "status": {
      "type": "belongsTo",
      "model": "SysCode",
      "foreignKey": "statusId"
    },
    "items": {
      "type": "referencesMany",
      "model": "ShipmentItem",
      "foreignKey": "itemsIds"
