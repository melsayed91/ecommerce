{
  "name": "sharedProduct",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "returnPeriode": {
      "type": "Number",
      "required": true,
      "default": 0
    },
    "warrantyPeriod": {
      "type": "Number",
      "required": true,
      "default": 0
    },
    "price": {
      "type": "Number",
      "required": true
    },
    "prototypePrice": {
      "type": "Number",
      "required": true
    },
    "moq": {
      "type": "Number",
      "required": true,
      "default": 1
    },
    "specs": {
      "type": [
        "object"
      ]
    },
    "discount": {
      "notes": "string",
      "sale_value": {
        "type": "Number",
        "required": true
      },
      "price": {
        "type": "Number",
        "required": true
      },
      "percentage": {
        "type": "Number",
        "required": true
      },
      "start_date": {
        "type": "Date",
        "required": true
      },
      "end_date": {
        "type": "Date",
        "required": true
      },
      "isActive": {
        "type": "Boolean",
        "required": true
      },
      "typeID": {
        "type": "Number",
        "required": true
      }
    },
    "stock": {
      "type": "Number",
      "required": true
    },
    "isActive": {
      "type": "Boolean",
      "default": true
    },
    "isDeleted": {
      "type": "Boolean",
      "default": false
    },
    "rating": {
      "type": "object"
    },
    "views": {
      "type": "Number",
      "default": 0
    },
    "sells": {
      "type": "Number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "accountId"
    },
    "category": {
      "type": "belongsTo",
      "model": "SysCode",
      "foreignKey": "categoryId"
    },
    "attachments": {
      "type": "referencesMany",
      "model": "attachment",
      "foreignKey": "attachmentIds",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "buyWith": {
      "type": "referencesMany",
      "model": "product",
      "foreignKey": "buyWithIds",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [
  ],
  "methods": {}
}