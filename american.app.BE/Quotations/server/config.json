{
  "restApiRoot": "/pluto",
  "host": "127.0.0.1",
  "port": 3333,
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "sharedMethods": {
      "*": false,
      "addRFQ": true,
      "addOffer": true,
      "getRFQs": true,
      "enableRFQ": true,
      "deleteRFQ": true,
      "addSpecification": true,
      "replyToSpecification": true,
      "getSpecifications": true
    }
  }
}
